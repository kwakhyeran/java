package oop.chap6.constructor;

import WorkBook.Student;

public class Test055{

	public static void main(String[] args) {
		
		Student stu1 = new Student();     //내가 Person이라는 클래스를 생성 => API클래스 사용법과 똑같다
		
		stu1.setName("kim");
		stu1.setKor(100);             //age가 private으로 선언되었으므로 접근할수없다.
		stu1.setEng(90);
		stu1.setMat(90);
		stu1.setSci(95);
		Student stu2 = new Student();     //내가 Person이라는 클래스를 생성 => API클래스 사용법과 똑같다
			
		stu2.setName("lee");
		stu2.setKor(50);             //age가 private으로 선언되었으므로 접근할수없다.
		stu2.setEng(40);
		stu2.setMat(30);
		stu2.setSci(35);
		
		Student stu3 = new Student();     //내가 Person이라는 클래스를 생성 => API클래스 사용법과 똑같다
		
		stu3.setName("park");
		stu3.setKor(80);             //age가 private으로 선언되었으므로 접근할수없다.
		stu3.setEng(70);
		stu3.setMat(80);
		stu3.setSci(45);
	
		stu1.print();
		stu2.print();
		stu3.print();
		
		/*
		System.out.println("Kim의 평균 :"+ stu1.getAvg() + "학점 :"+stu1.getGrade());
		System.out.println("lee의 평균 :" + stu2.getAvg()+ "학점 :"+stu2.getGrade());
		System.out.println("park의 평균 :" + stu3.getAvg()+ "학점 :"+stu3.getGrade());
*/
		

}}
