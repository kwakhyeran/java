package workbook60p;

import java.util.prefs.BackingStoreException;

public class MobileTest {

	public static void main(String[] args) {
	Ltab ltab =new Ltab("Ltab",500,"AP-01");
	Otab otab =new Otab("Otab",1000,"AND-20");

	System.out.println("Mobile"+"Battery"+"OS");
	System.out.println("----------------------");

	System.out.println("10분충전");
	System.out.println("Ltab"+ltab.getBatterySize()+ltab.getOsType());
	System.out.println("Otab"+otab.getBatterySize()+"AND-20");
	
	System.out.println("5분통화");
	System.out.println("Ltab"+ltab.getBatterySize()+"AP-01");
	System.out.println("Otab"+otab.getBatterySize()+"AND-20");
	}

}
