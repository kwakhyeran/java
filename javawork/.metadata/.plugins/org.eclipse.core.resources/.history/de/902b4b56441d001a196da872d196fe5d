package oop.chap07;

public class IceCream extends Dessert {

	private int cal;

	public IceCream() {
	}

	public IceCream(String productName, int price, int count, int cal) {
		super(productName, price, count);
		this.cal = cal;
	}

	public static void print() {

		System.out.println("============================");
		System.out.println("주문항목 " + "가격 " + "수량 " + "칼로리");
		System.out.println("============================");

	}

	public void printProductInfo() {
		System.out.println(getProductName() + getPrice() + getCount() + getcal());

	}

	public int getCal() {
		return cal;
	}

	public void setCal(int cal) {
		this.cal = cal;
	}

}