package oop.chap07.poly;

public class Beverage {

	private String name;
	int price;

	public Beverage(String name) {
		super();
		this.name = name;
	}

	public abstract void calPrice();

	public void print() {
		System.out.println();
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

}
