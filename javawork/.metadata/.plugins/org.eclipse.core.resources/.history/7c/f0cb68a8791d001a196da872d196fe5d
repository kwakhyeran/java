package oop.chap07.poly;

public class tea extends Beverage {
	private int amount;

	public tea() {
	}

	public tea(String name, int price, int amount) { // 持失切 莫失
		super(name, price);
		this.amount = amount;
	}


@Override
public void calcPrice() {
	if (name.equals("Americano")) {
		setPrice(1500);
	}
	if (name.equals("CafeLatte")) {
		setPrice(2500);
	}
	if (name.equals("Cappuccino")) {
		setPrice(3000);
	}
}

}