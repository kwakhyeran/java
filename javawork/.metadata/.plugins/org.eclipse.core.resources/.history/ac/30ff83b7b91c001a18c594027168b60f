package workbook60p;

public class MobileTest {

	public static void main(String[] args) {
	Ltab ltab =new Ltab("Ltab",500,"AP-01");
	Otab otab =new Otab("Otab",1000,"AND-20");

	System.out.print("Mobile"+"Battery"+"OS");
	System.out.print("----------------------");
	System.out.print(ltab.getMobileName()+ltab.getBatterySize()+ltab.getOsType());
	System.out.print(otab.getMobileName()+otab.getBatterySize()+otab.getOsType());
	System.out.println("Mobile"+"Battery"+"OS");
	System.out.println("----------------------");
	System.out.println("10분충전");
	System.out.println(ltab.getMobileName()+ltab.getBatterySize()+ltab.getOsType());
	System.out.println(otab.getMobileName()+otab.getBatterySize()+otab.getOsType());
	System.out.println("Mobile"+"Battery"+"OS");
	System.out.println("----------------------");
	System.out.println("5분통화");
	System.out.println(ltab.getMobileName()+ltab.getBatterySize()+ltab.getOsType());
	System.out.println(otab.getMobileName()+otab.getBatterySize()+otab.getOsType());
	}

}
