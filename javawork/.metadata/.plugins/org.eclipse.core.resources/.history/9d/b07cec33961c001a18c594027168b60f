package oop.chap07;

/*
 * <<상속관계에서 메소드가 갖는 특징>>
 * 1. 상속관계에서는 상위클래스에 정의된 멤버변수를 하위클래스에서 사용할수있다.
 */
class Parent {
	public void display() {

		System.out.println("부모의 display....");
	}
}

class Child extends Parent {
	public void test() {
		System.out.println("자식클래스의 test....");
		display();

	}

}

public class InheritanceTest02 {

	public static void main(String[] args) {
		Child obj = new Child();
		obj.test();
	}

}
